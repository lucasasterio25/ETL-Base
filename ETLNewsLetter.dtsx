    public override void Sender_ProcessInputRow(SenderBuffer Row)
    {
        string date = "";
        string email = "";
        if(Row.DATE_IsNull == true) {
            DateTime localDate = DateTime.Now; localDate = localDate.AddMonths(-6);
            date = localDate.GetDateTimeFormats()[57].Replace(" ", "T");
            email = Row.Email.ToString();
        } else {
            date = Row.DATE.GetDateTimeFormats()[57].Replace(" ", "T");
            email = Row.Email.ToString();
        }        

        Task<string> taskGetTokenAsync = GetTokenAsync(this.Variables.AppID.ToString(), this.Variables.AppSegredo.ToString());
        taskGetTokenAsync.Wait();

        Task taskGetSendMessages = GetSendMessages(taskGetTokenAsync.Result, date, email, Row.Link.ToString());
        taskGetSendMessages.Wait();
    }

