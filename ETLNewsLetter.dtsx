    public async Task<string> GetTokenAsync(string id, string secret)
    {
        var values = new Dictionary<string, string>
            {
                { "grant_type", "client_credentials" },
                { "client_id", id },
                { "client_secret", secret },
                { "resource", "https://graph.microsoft.com" }
            };

        var content = new FormUrlEncodedContent(values);

        HttpClient client = new HttpClient();
        var response = await client.PostAsync("https://login.microsoftonline.com/projetokroton3.onmicrosoft.com/oauth2/token", content);

        var responseString = await response.Content.ReadAsStringAsync();
        var Resp = new JavaScriptSerializer().DeserializeObject(responseString);
        var RespJson = Resp as Dictionary<string, object>;

        return RespJson["access_token"].ToString();
    }

    public async Task GetReceveMessages(string accessToken, string filter)
    {
        ArrayList members = new ArrayList();
        HttpClient client = new HttpClient();
        client.DefaultRequestHeaders.Add("Authorization", "Bearer " + accessToken);
        client.DefaultRequestHeaders.Add("Host", "graph.microsoft.com");
        HttpResponseMessage response = await client.GetAsync("https://graph.microsoft.com/v1.0/users?");//all users

        string retResp = await response.Content.ReadAsStringAsync();
        var Resp = new JavaScriptSerializer().DeserializeObject(retResp);
        var RespJson = Resp as Dictionary<string, object>;
        RespJson.Remove("@odata.context");

        foreach (var values in RespJson.Values)
        {
            foreach (var properties in (IEnumerable)values)
            {
                string phone = "";
                var item = properties as Dictionary<string, object>;
                foreach (var businessPhones in (IEnumerable)item["businessPhones"])
                {
                    phone = businessPhones.ToString();
                }

                members.Add(item["mail"].ToString());

                StgContactBuffer.AddRow();
                StgContactBuffer.id = IsNull(item["id"]);
                StgContactBuffer.displayName = IsNull(item["displayName"]);
                StgContactBuffer.jobTitle = IsNull(item["jobTitle"]);
                StgContactBuffer.mail = IsNull(item["mail"]);
                StgContactBuffer.mobilePhone = IsNull(item["mobilePhone"]);
                StgContactBuffer.officeLocation = IsNull(item["officeLocation"]);
                StgContactBuffer.businessPhones = phone;
                StgContactBuffer.EndOfRowset();
            }

        }
        await GetRecipients(accessToken, members, filter);
    }

    public async Task GetRecipients(string accessToken, ArrayList members, string filter)
    {
        DateTime localDate = DateTime.Now; localDate = localDate.AddMonths(-6);
        string date = localDate.GetDateTimeFormats()[57].Replace(" ", "T");

        for (int i = 0; i < members.Count; i++)
        {
            HttpClient client = new HttpClient();
            client.DefaultRequestHeaders.Add("Authorization", "Bearer " + accessToken);
            client.DefaultRequestHeaders.Add("Host", "graph.microsoft.com");
            string link = "https://graph.microsoft.com/v1.0/users/" + members[i].ToString() + "/mailFolders/AllItems/messages?$"+ filter+"sentDateTime%20ge%20" + date + "&$top=1000000000";
            HttpResponseMessage response = await client.GetAsync(link);
            string retResp = await response.Content.ReadAsStringAsync();
            var Resp = new JavaScriptSerializer().DeserializeObject(retResp);
            var RespJson = Resp as Dictionary<string, object>;
            RespJson.Remove("@odata.context");

            foreach (var values in RespJson.Values)
            {
                foreach (var properties in (IEnumerable)values)
                {
                    var item = properties as Dictionary<string, object>;

                    StgStatusBuffer.AddRow();
                    StgStatusBuffer.internetMessageId = IsNull(item["internetMessageId"]);
                    StgStatusBuffer.sentDateTime = DateTime.Parse(item["lastModifiedDateTime"].ToString());
                    StgStatusBuffer.isRead = IsRead(IsNull(item["isRead"]));
                    StgStatusBuffer.mail = IsNull(members[i]);
                    StgStatusBuffer.EndOfRowset();
                }

            }
        }
    }

