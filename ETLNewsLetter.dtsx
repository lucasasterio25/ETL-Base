                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @date DATETIME;
SET @date = (SELECT MAX(endtime) FROM [StgDwNewsLetter].[dbo].sysssislog
WHERE executionid NOT IN (SELECT DISTINCT executionid FROM [StgDwNewsLetter].[dbo].sysssislog
WHERE event = 'OnError' AND source IN ('Carga FactNewsLetter','Delete Stg','DimContact','DimGroup','DimMessage','DimSender','Insert Dim','Insert Stgs','PackageNewsLetter','StgContact','StgGroup','StgMessage','StgRecipient','StgStatus'))
AND executionid IN (SELECT DISTINCT executionid FROM [StgDwNewsLetter].[dbo].sysssislog
WHERE event = 'OnPostExecute' AND source IN ('Carga FactNewsLetter'))
)

DECLARE @count int, @i int, @output varchar(max)
SET @count = (SELECT COUNT(*) FROM [StgDwNewsLetter].[Dw].[GroupSender])
SET @i = 0
SET @output = '$filter=from/emailAddress/address%20eq%20'

WHILE @count&gt;=@i
BEGIN
IF(@count&lt;&gt;@i)
SET @output = CONCAT(@output, ''''+(SELECT [Email] FROM (
SELECT ROW_NUMBER() OVER(ORDER BY [Email] DESC) AS Row, [Email] FROM [StgDwNewsLetter].[Dw].[GroupSender]) AS A
WHERE Row = @i)+''''+'%20or%20from/emailAddress/address%20eq%20')
ELSE
SET @output = CONCAT(@output, ''''+(SELECT [Email] FROM (
SELECT ROW_NUMBER() OVER(ORDER BY [Email] DESC) AS Row, [Email] FROM [StgDwNewsLetter].[Dw].[GroupSender]) AS A 
WHERE Row = @i)+''''+ '%20and%20')

SET @i = @i + 1
END


SELECT [Email], @date as DATE, @output AS Link
  FROM [StgDwNewsLetter].[Dw].[UserSender]</property>
