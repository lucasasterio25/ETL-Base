                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT m.MessageId, c.ContactId, sedw.SenderId, gp.GroupId, dt.Date, s.[StatusId], cgp.[JobGroupId] ,CONCAT(m.MessageId, '#', c.ContactId) AS [ContactMessageId]
FROM [StgDwNewsLetter].[Dw].[StgStatus] ts
LEFT JOIN [StgDwNewsLetter].[Dw].[StgMessage] m ON ts.MessageId = m.MessageId
LEFT JOIN [StgDwNewsLetter].[Dw].[StgRecipient] rc ON rc.MessageId = m.MessageId AND rc.RecipientAddress IN (SELECT DISTINCT [GroupAddress] FROM [StgDwNewsLetter].[Dw].[StgGroup])
INNER JOIN [StgDwNewsLetter].[Dw].[StgGroup] gp ON gp.GroupAddress = rc.RecipientAddress
INNER JOIN [StgDwNewsLetter].[Dw].[StgContact] c ON c.Address = ts.Address
INNER JOIN [DwNewsLetter].[Dw].[DimDate] dt ON CONVERT(DATE,m.SentDateTime) = dt.Date
INNER JOIN [DwNewsLetter].[Dw].[DimSender] se ON m.FromAddress = se.Address
LEFT JOIN [DwNewsLetter].[Dw].[DimSender] sedw ON sedw.Address = se.Address
INNER JOIN [DwNewsLetter].[Dw].[DimStatus] s ON ts.IsRead = s.IsRead
INNER JOIN [StgDwNewsLetter].[Dw].[ContactJobGroup] cgpc ON c.ContactId = cgpc.ContactId
INNER JOIN [DwNewsLetter].[Dw].[DimJobGroup] cgp ON cgpc.[JobGroupId] = cgp.[JobGroupId]
UNION ALL
