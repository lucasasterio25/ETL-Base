    public static async Task<string> GetTokenAsync(string id, string secret)
    {
        var values = new Dictionary<string, string>
            {
                { "grant_type", "client_credentials" },
                { "client_id", id },
                { "client_secret", secret },
                { "resource", "https://graph.microsoft.com" }
            };

        var content = new FormUrlEncodedContent(values);

        HttpClient client = new HttpClient();
        var response = await client.PostAsync("https://login.microsoftonline.com/projetokroton3.onmicrosoft.com/oauth2/token", content);

        var responseString = await response.Content.ReadAsStringAsync();
        var Resp = new JavaScriptSerializer().DeserializeObject(responseString);
        var RespJson = Resp as Dictionary<string, object>;

        return RespJson["access_token"].ToString();
    }

    public async Task GetSendMessages(string accessToken, string date, string emailsender, string filter)
    {
        int top = await GetCountSentMessages(accessToken, emailsender);

        HttpClient client = new HttpClient();
        client.DefaultRequestHeaders.Add("Authorization", "Bearer " + accessToken);
        client.DefaultRequestHeaders.Add("Host", "graph.microsoft.com");
        HttpResponseMessage response = await client.GetAsync("https://graph.microsoft.com/v1.0/users/"+ emailsender + "/mailFolders/SentItems/messages?$top=" + top + "&"+filter+"sentDateTime%20ge%20" + date);

        string retResp = await response.Content.ReadAsStringAsync();
        var Resp = new JavaScriptSerializer().DeserializeObject(retResp);
        var RespJson = Resp as Dictionary<string, object>;
        RespJson.Remove("@odata.context");


        foreach (var values in RespJson.Values)
        {
            foreach (var properties in (IEnumerable)values)
            {
                var item = properties as Dictionary<string, object>;

                var Senders = item["sender"] as Dictionary<string, object>;
                string senderName = "";
                foreach (var sender in (IEnumerable)Senders.Values)
                {
                    var senderaddress = sender as Dictionary<string, object>;
                    senderName = senderaddress["name"].ToString();
                }

                string fromname = "";
                string fromaddress = "";
                var From = item["from"] as Dictionary<string, object>;
                foreach (var from in (IEnumerable)From.Values)
                {
                    var fromItem = from as Dictionary<string, object>;
                    fromname = fromItem["name"].ToString();
                    fromaddress = fromItem["address"].ToString();
                }


                StgMessageBuffer.AddRow();
                StgMessageBuffer.internetMessageId = IsNull(item["internetMessageId"]);
                StgMessageBuffer.sentDateTime = DateTime.Parse(item["sentDateTime"].ToString());
                StgMessageBuffer.subject = IsNull(item["subject"]);
                StgMessageBuffer.sender = senderName; //The account that is actually used to generate the message.

                StgMessageBuffer.from = fromname;//The mailbox owner and sender of the message.
                StgMessageBuffer.fromaddress = fromaddress;//The mailbox owner and sender of the message.

                StgMessageBuffer.importance = IsNull(item["importance"]);
                StgMessageBuffer.lastModifiedDateTime = DateTime.Parse(item["lastModifiedDateTime"].ToString());
                StgMessageBuffer.webLink = IsNull(item["webLink"]);
                StgMessageBuffer.EndOfRowset();

                var Recipients = item["toRecipients"];
                foreach (var email in (IEnumerable)Recipients)
                {
                    var objectadress = email as Dictionary<string, object>;
                    foreach (var property in (IEnumerable)objectadress)
                    {
                        var objectadres = objectadress as Dictionary<string, object>;
                        foreach (var objects in objectadres.Values)
                        {
                            var items = objects as Dictionary<string, object>;
                            StgRecipientBuffer.AddRow();
                            StgRecipientBuffer.MessageId = IsNull(item["internetMessageId"]);
                            StgRecipientBuffer.RecipientDisplayName = IsNull(items["name"]);
                            StgRecipientBuffer.RecipientAddress = IsNull(items["address"]);
                            StgRecipientBuffer.EndOfRowset();
                        }
                    }
                }




            }
        }
    }

    public async Task<int> GetCountSentMessages(string accessToken, string emailsender)
    {
        HttpClient client = new HttpClient();
        client.DefaultRequestHeaders.Add("Authorization", "Bearer " + accessToken);
        client.DefaultRequestHeaders.Add("Host", "graph.microsoft.com");
        HttpResponseMessage response = await client.GetAsync("https://graph.microsoft.com/v1.0/users/"+emailsender+"/mailFolders/SentItems?$count=true");

        string retResp = await response.Content.ReadAsStringAsync();
        var Resp = new JavaScriptSerializer().DeserializeObject(retResp);
        var RespJson = Resp as Dictionary<string, object>;

        int count = 0; Int32.TryParse(RespJson["totalItemCount"].ToString(), out count);
        count = count + 1;
        return count;
    }

    public static string IsNull(object campo)
    {
        if (campo == null)
        {
            return "";
        }
        else
        {
            return campo.ToString();
        }
    }
}
